{
  "base": "assets/semantic",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "dist/",
      "uncompressed": "dist/components/",
      "compressed": "dist/components/",
      "themes": "dist/themes/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "rtl": "No",
  "components": [
    "reset",
    "site",
    "button",
    "container",
    "divider",
    "header",
    "icon",
    "image",
    "input",
    "label",
    "list",
    "reveal",
    "segment",
    "form",
    "grid",
    "menu",
    "card",
    "item",
    "accordion",
    "checkbox",
    "dimmer",
    "dropdown",
    "embed",
    "rating",
    "shape",
    "sidebar",
    "sticky",
    "transition",
    "form",
    "state",
    "visibility"
  ],
  "version": "2.1.8"
}
